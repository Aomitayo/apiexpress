				"description":{"type":"string"},
				"errors":{"type":"array", "items":{"$ref":"ValidationError"}}
			}
		}
	},
	"apis": [
		{
			"path": "/hello",
			"description": "The collection of hello",
			"operations":[
				{
					"method": "GET",
					"summary": "Say hello",
					"nickname": "sayHello",
